{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/about/page.js", "src/app/layout.js/page.js", "src/app/envision/page.js", "src/app/page.js", "src/app/gallery/page.js", "src/app/playground/page.js", "src/app/inspire/page.js", "src/app/[lounge]/page.js", "src/app/tunes/page.js", "src/app/layout.tsx"],
  "exclude": ["node_modules"]
}
